     macro
          APPLY &n
          LEA.L      -$0C(sp_), A0
     if &n = 2 then
          MOVE.L     (sp_)+, (A0)+
     elseif &n = 3 then
          MOVE.L     (sp_)+, (A0)+
          MOVE.L     (sp_)+, (A0)+
     endif
