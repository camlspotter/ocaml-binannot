&DEBUG SETA &FINDSYM(&SYSGLOBAL, 'DEBUG')

 IF &DEBUG THEN
     machine mc68020
     print push,off
     include 'Types.a'
     print pop
 ENDIF

msg       record
          dc.b       'Fatal error in interp: bad opcode (%lx)', $0D, $00
endr

interprete proc export

accu_                    equ    A2
pc_                      equ    A3
sp_                      equ    A4
